pipeline {
    agent any

    stages {
        stage('clean') {
            steps {
                sh 'mvn clean'
            }
        }
        stage('package') {
            steps {
                sh 'mvn package -DskipTests'
            }
        }
        stage('deploy'){
        	steps{
        		sh 'cp /home/ec2-user/.jenkins/workspace/Reimbursement/target/Project1-0.0.1-SNAPSHOT.war /home/ec2-user/apache-tomcat-9.0.39/webapps'
        	}
        }
        stage('start server') {
            steps {
                sh 'sh /home/ec2-user/apache-tomcat-8.5.58/bin/startup.sh'
            }
        }
    }
}
